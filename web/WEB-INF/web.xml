<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>reg</servlet-name>
        <servlet-class>servlet.Reg</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reg</servlet-name>
        <url-pattern>/reg</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>leave</servlet-name>
        <servlet-class>servlet.Leave</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>leave</servlet-name>
        <url-pattern>/leave</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>reply</servlet-name>
        <servlet-class>servlet.Reply</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reply</servlet-name>
        <url-pattern>/reply</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>servlet.Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delete_rep</servlet-name>
        <servlet-class>servlet.Delete_rep</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete_rep</servlet-name>
        <url-pattern>/delete_rep</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>top</servlet-name>
        <servlet-class>servlet.Top</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>top</servlet-name>
        <url-pattern>/top</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>cancelTop</servlet-name>
        <servlet-class>servlet.CancelTop</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>cancelTop</servlet-name>
        <url-pattern>/cancelTop</url-pattern>
    </servlet-mapping>

    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.html</url-pattern>
            <el-ignored>true</el-ignored>
            <page-encoding>UTF-8</page-encoding>
            <scripting-invalid>false</scripting-invalid>
        </jsp-property-group>
    </jsp-config>

    <servlet>
        <servlet-name>replyre</servlet-name>
        <servlet-class>servlet.Replyre</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>replyre</servlet-name>
        <url-pattern>/replyre</url-pattern>
    </servlet-mapping>

    <servlet>
       <servlet-name>delete_rerep</servlet-name>
        <servlet-class>servlet.Delete_rerep</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete_rerep</servlet-name>
        <url-pattern>/delete_rerep</url-pattern>
    </servlet-mapping>
    <servlet>
       <servlet-name>nextpage</servlet-name>
        <servlet-class>servlet.NextPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>nextpage</servlet-name>
        <url-pattern>/nextpage</url-pattern>
    </servlet-mapping>
    <servlet>
       <servlet-name>quit</servlet-name>
        <servlet-class>servlet.Quit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>quit</servlet-name>
        <url-pattern>/quit</url-pattern>
    </servlet-mapping>
    <servlet>
       <servlet-name>saveInfo</servlet-name>
        <servlet-class>servlet.SaveInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>saveInfo</servlet-name>
        <url-pattern>/saveInfo</url-pattern>
    </servlet-mapping>

</web-app>
